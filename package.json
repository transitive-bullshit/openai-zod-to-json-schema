{
  "name": "openai-zod-to-json-schema",
  "version": "0.0.1",
  "description": "Convert Zod schemas to JSON schemas which are optionally compatible with OpenAI's structured outputs.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transitive-bullshit/agentic.git"
  },
  "packageManager": "pnpm@9.7.0",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "license",
    "readme.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "del clean",
    "prebuild": "run-s clean vendor",
    "predev": "run-s clean",
    "pretest": "run-s build",
    "test": "run-s test:*",
    "test:format": "prettier --check \"**/*.{js,ts,tsx}\"",
    "test:lint": "eslint .",
    "test:typecheck": "tsc --noEmit",
    "test:unit": "vitest run",
    "prevendor": "del src/vendor",
    "vendor": "mkdir -p src/vendor/zod-to-json-schema && cp -r node_modules/openai/src/_vendor/zod-to-json-schema src/vendor",
    "release": "np",
    "precommit": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "devDependencies": {
    "@fisch0920/eslint-config": "^1.4.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.1.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.4",
    "json-schema": "^0.4.0",
    "lint-staged": "^15.2.8",
    "local-ref-resolver": "^0.2.0",
    "npm-run-all2": "^6.2.2",
    "only-allow": "^1.2.1",
    "openai": "^4.55.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "2.0.5",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --ignore-unknown --write",
      "eslint --fix"
    ]
  }
}
