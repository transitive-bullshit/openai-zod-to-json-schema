{
  "name": "openai-zod-to-json-schema",
  "version": "1.0.2",
  "description": "Convert Zod schemas to JSON schemas which are optionally compatible with OpenAI's structured outputs.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transitive-bullshit/openai-zod-to-json-schema.git"
  },
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "del clean",
    "prebuild": "run-s clean vendor",
    "predev": "run-s clean",
    "pretest": "run-s build",
    "test": "run-s test:*",
    "test:format": "prettier --check \"**/*.{js,ts,tsx}\"",
    "test:lint": "eslint .",
    "test:typecheck": "tsc --noEmit",
    "test:unit": "vitest run",
    "prevendor": "del src/vendor",
    "vendor": "mkdir -p src/vendor/zod-to-json-schema && cp -r node_modules/openai/src/_vendor/zod-to-json-schema src/vendor",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@fisch0920/eslint-config": "^1.4.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.7.5",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "del-cli": "^6.0.0",
    "eslint": "^8.57.1",
    "json-schema": "^0.4.0",
    "local-ref-resolver": "^0.2.0",
    "npm-run-all2": "^6.2.3",
    "only-allow": "^1.2.1",
    "openai": "^4.67.3",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "2.1.3",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  }
}
